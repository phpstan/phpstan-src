# All stuff in bleedingEdge gets into the default set when a new major version is out.
parameters:
	featureToggles:
		bleedingEdge: true
		closureUsesThis: true
		randomIntParameters: true
		nullCoalesce: true
		fileWhitespace: true
		unusedClassElements: true
		readComposerPhpVersion: true
		dateTimeInstantiation: true
	stubFiles:
		- ../stubs/SplObjectStorage.stub

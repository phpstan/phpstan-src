{
	"name": "phpstan/issue-bot",
    "require": {
		"php": "^8.3",
		"guzzlehttp/guzzle": "^7.5",
		"knplabs/github-api": "^3.9",
		"league/commonmark": "^2.3",
		"nette/neon": "^3.3",
		"nette/utils": "^4.0",
		"phpstan/phpstan-strict-rules": "1.6.x-dev",
		"symfony/console": "^7.0",
		"symfony/finder": "^7.0"
	},
	"autoload": {
		"psr-4": {
			"PHPStan\\IssueBot\\": [
				"src/"
			]
		}
	},
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"php-http/discovery": true
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require-dev": {
		"phpunit/phpunit": "^11.0"
	}
}

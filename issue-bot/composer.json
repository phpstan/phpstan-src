{
	"name": "phpstan/issue-bot",
    "require": {
		"php": "^8.1",
		"guzzlehttp/guzzle": "^7.5",
		"knplabs/github-api": "^3.9",
		"league/commonmark": "^2.3",
		"nette/neon": "^3.3",
		"nette/utils": "^3.2",
		"phpstan/phpstan-strict-rules": "^1.4",
		"symfony/console": "^6.1",
		"symfony/finder": "^6.1"
	},
	"autoload": {
		"psr-4": {
			"PHPStan\\IssueBot\\": [
				"src/"
			]
		}
	},
	"config": {
		"sort-packages": true,
		"platform": {
			"php": "8.1.99"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5"
	}
}
